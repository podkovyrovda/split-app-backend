{
  "type": "mongodb",
  "host": "localhost",
  "port": 27017,
  "database": "routiin",
  "entities": [
    "dist/**/*.orm-entity{.ts,.js}"
  ],
  "autoLoadEntities": true,
  "synchronize": true,
  "migrations": [
    "dist/modules/**/migrations/*{.ts,.js}"
  ],
  "cli": {
    "migrationsDir": "migrations"
  }
}
